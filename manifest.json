{
    "manifest_version": 2,
    "name": "Banana",
    "description": "More accurately track code snippets in GitHub PR diffs.",
    "version": "0.0.0",
    "content_scripts": [
        {
            "matches": ["https://*/*/pull/*/files",
                        "https://*/*/*/pull/*/files"],
            "js": ["jquery-1.9.1.min.js", "banana.js"],
            "css": ["banana.css"]
        }
    ],
    "web_accessible_resources": [
        "jquery-1.9.1.min.js"
    ]
}
